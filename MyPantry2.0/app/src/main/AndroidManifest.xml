<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.z.mypantry20">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyPantryView"
            android:label="@string/title_activity_item_view"
            android:theme="@style/AppTheme"/>
        <activity android:exported="true"
            android:name=".CategoryOverviewView"
            android:label="@string/title_activity_category_view"
            android:theme="@style/Theme.AppCompat.Light" />
        <activity android:name=".CategoryView" />
        <activity android:name=".RecipeView"></activity>
    </application>

</manifest>